{
  "manifest_version": 3,
  "name": "TabTamer",
  "version": "1.0",
  "description": "Sort tabs by domain name",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}